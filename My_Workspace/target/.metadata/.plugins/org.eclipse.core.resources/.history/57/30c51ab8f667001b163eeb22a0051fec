#include <stdio.h>


int main(){

	char a, b, c, d ,e, f;

	printf("Enter the 6 characters of your choice:");
	scanf("%c %c %c %c %c %c", &a, &b, &c, &d, &e, &f);

	printf("ASCII Codes: %d, %d, %d, %d, %d, %d \n", a,b,c,d,e,f);


	while(getchar() != "\n"){



	}

	printf("Please enter the keyboard to exit. ");
	getchar();




	return 0;
}
